<div class="h2about">Users Directory</div>
<br>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% @users.each do |user| %>
      <p id="hind"><%= link_to user.username, user %>
        <small><em>(member since: <%= current_user.created_at.strftime("%B %d, %Y") %>)</em></small>
      </p>
    <% end %>
  </div>
</div>
